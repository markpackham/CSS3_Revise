{
  "manifest_version": 2,

  "name": "Traversy Launcher",
  "description": "Quick launch Traversy Media",
  "version": "1.0",
  "icons": { "128": "icons/icon_128.png" },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
